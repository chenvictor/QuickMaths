{
  "name": "Quick Math",
  "description": "Add math inputs to the current web page",
  "version": "0.0.2",
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "web_accessible_resources": [
    "ui/modal.html",
    "ui/bar.html",
    "js/quick-maths.js",
    "js/jquery-3.3.1.min.js"
  ],
  "options_page": "options/index.html",
  "browser_action": {
    "default_title": "Insert easy math input"
  },
  "manifest_version": 2
}
